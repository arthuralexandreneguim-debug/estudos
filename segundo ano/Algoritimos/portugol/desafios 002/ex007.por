programa {
real valor, dobro, tercaparte


  funcao inicio() {

    //entrada
      escreva("digite algum valor: ")
      leia(valor)

    //processamento
      dobro=valor*2
      tercaparte=valor/3

    //saida
      escreva("a terça parte de ", valor, " é ", tercaparte, " \n")
      escreva("o dobro ", valor, " é ", dobro)
  }
}
