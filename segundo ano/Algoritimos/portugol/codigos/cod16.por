programa {
  inteiro contador
  funcao inicio() {
 // 1. Inicialização
        inteiro contador = 1

        escreva("Iniciando a contagem...\n")

        // 2. Condição
        enquanto (contador <= 20) {
            // 3. Ação
            escreva(contador, "\n")

            // 4. Atualização
            contador = contador + 3
        }

        escreva("Contagem finalizada!\n")















  }
}
